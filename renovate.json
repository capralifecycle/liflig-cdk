{
  "extends": ["github>capraconsulting/renovate-config:default"],
  "automerge": true,
  "automergeType": "branch",
  "lockFileMaintenance": {
    "automerge": true,
    "automergeType": "branch"
  },
  "packageRules": [
    {
      "sourceUrlPrefixes": [
        "https://github.com/aws/aws-cdk"
      ],
      "depTypeList": [
        "devDependencies"
      ],
      "rangeStrategy": "replace"
    }
  ]
}
