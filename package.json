    "prepare": "npm run build && husky install",
    "semantic-release": "semantic-release",
    "snapshots": "./scripts/create-snapshots.sh"
  },
  "bin": {
    "cdk-create-snapshots": "lib/bin/cdk-create-snapshots.js"
  },
  "keywords": [
    "cdk"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "assets/**/*",
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.102.0",
