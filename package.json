    "@aws-cdk/aws-stepfunctions-tasks": "1.102.0",
    "@aws-cdk/core": "1.102.0",
    "@aws-cdk/custom-resources": "1.102.0",
    "@aws-cdk/pipelines": "1.102.0",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/aws-lambda": "8.10.76",
    "@types/jest": "26.0.23",
    "@types/node": "13.13.52",
