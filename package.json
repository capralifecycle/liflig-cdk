{
  "name": "@liflig/cdk",
  "version": "0.0.0-development",
  "description": "CDK library for Liflig",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/capralifecycle/liflig-cdk"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build && husky",
    "semantic-release": "semantic-release",
    "snapshots": "./scripts/create-snapshots.sh",
    "docs": "typedoc src --out docs"
  },
  "bin": {
    "cdk-create-snapshots": "lib/bin/cdk-create-snapshots.js",
    "fetch-pipeline-variables": "lib/bin/fetch-pipeline-variables.js"
  },
  "keywords": [
    "cdk"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "assets",
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "semantic-release": {
      "@semantic-release/npm": "12.0.1"
    }
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.68.0",
    "@aws-sdk/client-cloudwatch-logs": "3.750.0",
    "@aws-sdk/client-codebuild": "3.753.0",
    "@aws-sdk/client-codepipeline": "3.752.0",
    "@aws-sdk/client-ecs": "3.752.0",
    "@aws-sdk/client-s3": "3.750.0",
    "@aws-sdk/client-secrets-manager": "3.750.0",
    "@aws-sdk/client-ses": "3.750.0",
    "@aws-sdk/client-sesv2": "3.753.0",
    "@aws-sdk/client-sfn": "3.750.0",
    "@aws-sdk/lib-storage": "3.750.0",
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.21.0",
    "@types/aws-lambda": "8.10.147",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.5",
    "@typescript-eslint/eslint-plugin": "8.25.0",
    "@typescript-eslint/parser": "8.25.0",
    "aws-cdk": "2.1000.2",
    "aws-cdk-lib": "2.180.0",
    "constructs": "10.4.2",
    "esbuild": "0.25.0",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-cdk-snapshot": "2.2.5",
    "prettier": "3.5.2",
    "semantic-release": "24.2.3",
    "ts-jest": "29.2.6",
    "tsx": "4.19.3",
    "typedoc": "0.27.8",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "3.750.0",
    "@capraconsulting/webapp-deploy-lambda": "2.4.1",
    "aws-jwt-verify": "5.0.0",
    "del": "8.0.0",
    "glob": "11.0.1",
    "source-map-support": "0.5.21"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  }
}
