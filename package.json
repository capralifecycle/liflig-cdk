{
  "name": "@liflig/cdk",
  "version": "0.0.0-development",
  "description": "Experimental CDK library for Liflig",
  "repository": {
    "type": "git",
    "url": "https://github.com/capralifecycle/liflig-cdk"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
