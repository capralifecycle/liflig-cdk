{
  "name": "@liflig/cdk",
  "version": "0.0.0-development",
  "description": "Experimental CDK library for Liflig",
  "repository": {
    "type": "git",
    "url": "https://github.com/capralifecycle/liflig-cdk"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "snapshots": "./scripts/create-snapshots.sh"
  },
  "bin": {
    "cdk-create-snapshots": "lib/bin/cdk-create-snapshots.js",
    "cdk-jenkinsfile-check-referenced-stacks": "lib/bin/cdk-jenkinsfile-check-referenced-stacks.js"
  },
  "keywords": [
    "cdk"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.63.0",
    "@aws-cdk/aws-codebuild": "1.63.0",
    "@aws-cdk/aws-ecs": "1.63.0",
    "@aws-cdk/aws-iam": "1.63.0",
    "@aws-cdk/aws-lambda": "1.63.0",
    "@aws-cdk/aws-s3": "1.63.0",
    "@aws-cdk/core": "1.63.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/aws-lambda": "8.10.62",
    "@types/jest": "26.0.14",
    "@types/node": "13.13.21",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "aws-cdk": "1.63.0",
    "aws-sdk": "2.686.0",
    "eslint": "7.9.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-deprecation": "1.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jest-cdk-snapshot": "1.4.0",
    "prettier": "2.1.2",
    "semantic-release": "17.1.2",
    "ts-jest": "26.4.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "dependencies": {
    "@capraconsulting/webapp-deploy-lambda": "1.1.1",
    "cpy": "^8.1.0",
    "del": "^5.1.0",
    "execa": "^4.0.2",
    "glob": "7.1.6",
    "source-map-support": "^0.5.19"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "@aws-cdk/aws-codebuild": "^1.38.0",
    "@aws-cdk/aws-ecs": "^1.38.0",
    "@aws-cdk/aws-iam": "^1.38.0",
    "@aws-cdk/aws-lambda": "^1.38.0",
    "@aws-cdk/aws-s3": "^1.38.0",
    "@aws-cdk/core": "^1.38.0"
  }
}
